# GIMP gimprc
# 
# This is your personal gimprc file.  Any variable defined in this file takes
# precedence over the value defined in the system-wide gimprc:
# /etc/gimp/2.0/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(default-image
    (width 1920)
    (height 1080)
    (unit pixels)
    (fill-type transparent)
    (comment ""))
(save-document-history no)
(import-add-alpha yes)
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(image-title-format "%D*%f %wx%h")
(image-status-format "%n (%m, %t, %o, %L)")
(default-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers yes)
    (show-scrollbars yes)
    (show-selection yes)
    (show-layer-boundary yes)
    (show-canvas-boundary yes)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides yes)
    (snap-to-grid no)
    (snap-to-canvas yes)
    (snap-to-path yes)
    (padding-mode custom)
    (padding-color (color-rgb 0.13333333333333333 0.13333333333333333 0.13333333333333333)))
(default-fullscreen-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers yes)
    (show-scrollbars yes)
    (show-selection yes)
    (show-layer-boundary yes)
    (show-canvas-boundary yes)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides yes)
    (snap-to-grid no)
    (snap-to-canvas yes)
    (snap-to-path yes)
    (padding-mode custom)
    (padding-color (color-rgb 0.13333333333333333 0.13333333333333333 0.13333333333333333)))
(move-tool-changes-active yes)
(filter-tool-use-last-settings yes)
(save-session-info no)
(save-tool-options no)
(toolbox-wilber no)
(theme "System")
(icon-size small)
(action-history-size 0)
(user-manual-online yes)
(color-profile-policy convert)
(layer-add-mask-type alpha-transfer)
(selection-feather-radius 2)
(stroke-options
    (method paint-method))


# end of gimprc
